{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "endOfLine": "auto"
      }
    ],
    "no-unused-vars": "warn",
    "no-console": "warn",
    "func-names": "error",
    "no-process-exit": "error",
    "object-shorthand": "error",
    "class-methods-use-this": "error"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
